{
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/index.js" },
    { "source": "/trip/:shareId", "destination": "/web/trip.html" },
    { "source": "/:path*", "destination": "/web/:path*" }
  ],
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/web/**",
      "use": "@vercel/static"
    }
  ]
}