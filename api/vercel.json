{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/trip/(.*)", "dest": "/api/trip.html" },
    { "src": "/trip", "dest": "/api/trip.html" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}